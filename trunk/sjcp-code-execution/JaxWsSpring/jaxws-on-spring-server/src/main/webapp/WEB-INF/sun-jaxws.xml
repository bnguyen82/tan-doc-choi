<?xml version="1.0" encoding="UTF-8"?>
<endpoints 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/jax-ws/ri/runtime http://java.sun.com/webservices/docs/2.0/jaxws/sun-jaxws.xsd"
    xmlns="http://java.sun.com/xml/ns/jax-ws/ri/runtime" version="2.0">

	
    <endpoint 
    	implementation="de.gmorling.jaxwsonspring.shop.products.ProductsPort"
        name="Products" 
        url-pattern="/Products"
        wsdl="WEB-INF/wsdl/products.wsdl"
        service="{http://www.gmorling.de/jaxwsonspring/shop/products}ProductsService"
        port="{http://www.gmorling.de/jaxwsonspring/shop/products}ProductsPort" />
	
    <endpoint 
    	implementation="de.gmorling.jaxwsonspring.shop.monitor.MonitorPort"
        name="Monitor" 
        url-pattern="/Monitor"
        wsdl="WEB-INF/wsdl/monitor.wsdl"
        service="{http://www.gmorling.de/jaxwsonspring/shop/monitor}MonitorService"
        port="{http://www.gmorling.de/jaxwsonspring/shop/monitor}MonitorPort" />	

</endpoints>